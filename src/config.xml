<?xml version="1.0" encoding="UTF-8"?>
<Config>
    <ProgramType>Normal</ProgramType>
    <!-- <ProgramType lag="10">Time Series</ProgramType> -->
    <Iterations>10</Iterations>
    <Generations>20</Generations>
    <StoppingCriteria>0.0001</StoppingCriteria>
    <FitnessCases file="pythagorean_theorem.csv" />
    <CrossoverProb>0.7</CrossoverProb>
    <MutationProb>0.1</MutationProb>
    <HoistMutationProb>0.1</HoistMutationProb>
    <!-- <Seed>0</Seed> -->

    <!-- delete or comment-out the Functions that you don't require -->
    <AllowedFunctions>
        <Function>Addition</Function>
        <Function>Subtraction</Function>
        <Function>Multiplication</Function>
        <Function>Division</Function>
        <Function>Square Root</Function>
        <!-- <Function>Sine</Function> -->
        <!-- <Function>Cosine</Function> -->
        <!-- <Function>Exponential</Function> -->
        <!-- <Function>Logarithm</Function> -->
    </AllowedFunctions>

    <Population>
        <Size>500</Size>
        <MinInitTreeSize>15</MinInitTreeSize>

        <!-- If AlwaysReplaceParents is uncommented parents are always replaced by children, even if they are less fit. -->
        <!-- <AlwaysReplaceParents /> -->
    </Population>
</Config>
